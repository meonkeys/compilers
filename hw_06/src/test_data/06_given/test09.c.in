/*Testcase 9: Insertion sort */

int insertion_sort(int arr[], int size)
{
  int i;
  for (i=1; i < size; i = i+1)
    {
      int j, temp;
      temp = arr[i];
      for (j=i; j > 0 && temp < arr[j-1]; j = j-1)
	arr[j] = arr[j-1];
      arr[j] = temp;
    }
  return 1;
}



int main()
{
 int array[20];
 int i;
 
 for (i =0; i < 20; i = i + 2)
   {
     array[i] = 100-i;
     array[i+1] = 10+i*2;
   }

 write("The Unsorted array = ");

 for (i =0; i < 20; i = i + 1)
   write(array[i]);
 write("\n");
 
 insertion_sort(array, 20);

 write("The sorted array = ");
 for (i =0; i < 20; i = i + 1)
   write(array[i]);
 write("\n");
 
 return 1;
}

