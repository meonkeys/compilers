/* Testcase 3: Test prime numbers for for and if controls */

int is_prime(int n) 
{
  int k, limit;
  if (n == 2) 
    return 1;
  if ((n - (n/2 * 2))== 0) 
    return 0; 
  limit = n / 2;
 
  for ( k = 3; k <= limit; k = k + 2)
    if ((n - (n/k * k)) == 0)
      return 0;
  return 1;
}

/* main program */
int main()
{
  int i, num1, num2;
  
  write("enter a range, for example, 800 900 ");
  num1 = read();
  num2 = read();
 
  for (i =num1; i < num2; i = i + 1)
    {
      if (is_prime(i))
	write (i);
    } 
  return 1; 
}

