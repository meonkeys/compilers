/* Can you handle WHILE and IF statements ? */

int MatrixMultiplication(int V1[][10], int V2[][10])
{
 int result[10][10];          /*multi-dimensional array declaration */

/* for is used like this */
   for (i = 0; i < 10; i = i + 1) 
   {
      for(j = 0; j < 10; j = j + 1) 
      {
            result[i][j] = 0;
            for (k = 0; k < 10; k = k + 1) 
            {
				result[i][j] = result[i][j] + V1[i][k]*V2[k][j];
            }
      }
   }

  return 1;
}

