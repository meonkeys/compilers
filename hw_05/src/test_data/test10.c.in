/* TestCase 10: Recursive procedure call */ 

int result;
int num;

int fact()
{
  int localnum;
	
  localnum = num; 
  if (localnum == 1)
    {
		write("local: ");
		write(localnum);
		write("\n");
      return localnum;
    }
  else
    {
      num = num - 1;
	  write("num: ");
	  write(num);
	  write("\n");
      return ( localnum * fact( ) );
	  return result;
    }
}

void main()
{
  
  write("Enter a number:");
  num = read();

  if (num > 1)
    {
		write("num > 1\n");	
      result = fact();
	  write("result: ");
	  write(result);
	  write("\n");
    }
  else
    {
		write("else\n");
      result = 1;
    }
  write("The factorial is ");
  write(result);
  write("\n");
}

