/*TestCase 5: Simple while statement */

void main()
{
 float b,c,d;

 b = fread();
 c = fread();

 d = 1.1;
 write("d: ");
 write(d);
 write("\n");

 while( b < c)
 {
  d = d + b;
  write("d: ");
  write(d);
  write("\n");
  c = c - 1.0;
  write("c: ");
  write(c);
  write("\n");
 }

 write("The answer is ");
 write (d);
 write ("\n");
}


/*
Expected Output (for input b = 1.3, c = 5.2): The answer is 6.30000
*/
